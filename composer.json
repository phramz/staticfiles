{
  "name": "phramz/staticfiles-middleware",
  "license": "MIT",
  "type": "library",
  "keywords": ["staticfiles", "stack", "stackphp", "middleware"],
  "description": "This middleware will serve static files from a local directory",
  "authors": [
    {
      "name": "Maximilian Reichel",
      "email": "info@phramz.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/http-foundation": "~2.1",
    "symfony/http-kernel": "~2.1",
    "webmozart/path-util": "^2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1",
    "symfony/var-dumper": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "Phramz\\Staticfiles\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phramz\\Staticfiles\\Tests\\": "tests/"
    }
  }
}
